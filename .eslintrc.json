{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [ "warn", "windows" ],
        "spaced-comment": [ "warn", "always" ],
        "require-jsdoc": [ "warn" ],
        "object-curly-spacing": [ "warn", "always" ]
    }
}
